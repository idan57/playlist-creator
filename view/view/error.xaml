<Window x:Class="view.error"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:view"
        mc:Ignorable="d"
        Title="error" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="small_window.png"/>
        </Grid.Background>
        <Label x:Name="errMsg" Content="Label" HorizontalAlignment="Center" Height="91" Margin="0,256,0,0" VerticalAlignment="Top" Width="348" Background="White" FontSize="18"/>

    </Grid>
</Window>
